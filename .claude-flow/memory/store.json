    "research/neon-architecture": {
      "key": "research/neon-architecture",
      "value": {
        "components": {
          "safekeeper": {
            "role": "WAL durability layer",
            "protocol": "Paxos",
            "function": "Receives WAL from compute, stores until pageserver processes"
          },
          "pageserver": {
            "role": "Storage engine",
            "language": "Rust",
            "function": "Processes WAL into custom format, serves GetPage@LSN requests, uploads to S3"
          },
          "compute": {
            "role": "Query execution",
            "base": "Standard PostgreSQL",
            "function": "Runs queries, streams WAL to safekeepers"
          },
          "storage_controller": {
            "role": "Orchestration",
            "function": "Manages sharding and tenant placement"
          }
        },
        "key_functions": {
          "GetPage_at_LSN": "Reconstructs any page at any point in time by finding last image + applying WAL records"
        },
        "storage_model": {
          "layers": [
            "InMemory (WAL reorder buffer)",
            "L0 DeltaLayer (flushed from memory)",
            "ImageLayer (key snapshots)",
            "Cloud object storage (S3)"
          ]
        }
      },
      "metadata": {
        "category": "research",
        "topic": "neon-architecture"
      },
