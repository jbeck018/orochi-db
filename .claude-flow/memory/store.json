    "research/branching-tools": {
      "key": "research/branching-tools",
      "value": {
        "tools": [
          {
            "name": "Neon Storage Engine",
            "repo": "github.com/neondatabase/neon",
            "language": "Rust",
            "approach": "Full compute/storage separation with Pageserver + Safekeeper",
            "pros": [
              "Production ready (GA Aug 2024)",
              "True instant branching",
              "Scale to zero",
              "Time travel queries"
            ],
            "cons": [
              "Complete architecture change",
              "Network latency vs local NVMe",
              "Complex to self-host"
            ]
          },
          {
            "name": "pg_branch",
            "repo": "github.com/NAlexPear/pg_branch",
            "language": "Rust (Postgres extension)",
            "approach": "BTRFS snapshot-based branching",
            "pros": [
              "Simple Postgres extension",
              "Works with existing PG",
              "Fast on BTRFS"
            ],
            "cons": [
              "Pre-alpha/experimental",
              "Requires BTRFS filesystem",
              "Limited to single cluster"
