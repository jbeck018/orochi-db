    "research/orioledb-overview": {
      "key": "research/orioledb-overview",
      "value": {
        "name": "OrioleDB",
        "type": "PostgreSQL Storage Extension",
        "status": "Public Beta",
        "license": "Apache 2.0 / PostgreSQL License",
        "features": [
          "Index-organized tables",
          "MVCC with UNDO log (no bloat)",
          "Copy-on-write checkpoints",
          "Row-level WAL",
          "Block-level zstd compression",
          "No buffer mapping (lock-less page reading)",
          "Designed for distributed/multimaster"
        ],
        "performance": "3.3x speedup on TPC-C vs heap",
        "limitations": [
          "Only B-tree indexes supported",
          "No pg_vector HNSW indexes yet",
          "Requires patches to PostgreSQL core",
          "Not production ready"
        ],
        "relevance_to_orochi": "Could complement our HTAP extension with better write performance and no bloat"
      },
      "metadata": {
        "category": "research",
        "topic": "storage-engines"
      },
      "storedAt": "2026-01-23T22:33:49.822Z",
      "accessCount": 0,
      "lastAccessed": "2026-01-23T22:33:49.822Z"
    },
    "research/database-branching": {
      "key": "research/database-branching",
      "value": {
        "topic": "Instant Database Branching",
        "approaches": {
          "neon": {
            "method": "Copy-on-write at storage layer",
            "architecture": "Separated compute and storage",
            "speed": "Milliseconds regardless of DB size",
            "storage_efficiency": "Only stores delta/changes",
            "technical_basis": "Custom multi-tenant storage system with CoW paging layer"
          },
          "supabase": {
            "method": "Schema-only branching",
            "architecture": "Traditional PostgreSQL",
            "speed": "Requires provisioning new DB + running migrations",
            "storage_efficiency": "Full copy for each branch",
            "limitation": "Need seed script for data"
          },
          "xata": {
            "method": "Copy-on-write at storage layer",
            "speed": "Instant like Neon",
            "storage_efficiency": "Only stores delta/changes"
          }
        },
        "key_insight": "CoW requires custom storage layer that separates compute from storage"
      },
      "metadata": {
        "category": "research",
        "topic": "database-branching"
      },
      "storedAt": "2026-01-23T22:33:50.279Z",
